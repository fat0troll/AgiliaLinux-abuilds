#----------------------------- Make PKG --------------------------------------
#-------------------------- Сборка пакета ------------------------------------

#ran before function build()
#запускается перед сборкой
#before_build()
#{
#echo ""
#}

build()
{
#make dirs for packages
mkdir -p $startdir/libreoffice_package_{af,ar,as,be-BY,bg,bn,bo,br,brx,bs,ca,cs,cy,da,de,dgo,dz,el,en-GB,en-ZA,eo,es,et,eu,fa,fi,fr,ga,gd,gl,gu,he,hi,hr,hu,is,it,ja,ka,kk,km,kn,ko,kok,ks,ku,ky,lo,lt,lv,mai,mk,ml,mn,mni,mr,ms,my,ns,oc,om,or,pa-IN,pap,pl,ps,pt,ro,ru,rw,sa-IN,sat,sd,sh,si,sk,sl,sq,sr,ss,st,sv,sw-TZ,ta,te,tg,th,ti,tn,tr,ts,ug,uk,ur,uz,ve,vi,xh,zh-CN,zh-TW,zu}/usr/lib${LIBDIRSUFFIX}/libreoffice/basis-link
#make temp builddir
mkdir -p $startdir/build/{af,ar,as,be-BY,bg,bn,bo,br,brx,bs,ca,cs,cy,da,de,dgo,dz,el,en-GB,en-ZA,eo,es,et,eu,fa,fi,fr,ga,gd,gl,gu,he,hi,hr,hu,is,it,ja,ka,kk,km,kn,ko,kok,ks,ku,ky,lo,lt,lv,mai,mk,ml,mn,mni,mr,ms,my,ns,oc,om,or,pa-IN,pap,pl,ps,pt,ro,ru,rw,sa-IN,sat,sd,sh,si,sk,sl,sq,sr,ss,st,sv,sw-TZ,ta,te,tg,th,ti,tn,tr,ts,ug,uk,ur,uz,ve,vi,xh,zh-CN,zh-TW,zu}
cd $srcache
# mkpkg doesn't support recursive download. Yet.
wget -c $mirror/$ver/rpm/x86/LibO_${lover}_Linux_x86_{langpack,helppack}-rpm_{af,ar,as,be-BY,bg,bn,bo,br,brx,bs,ca,cs,cy,da,de,dgo,dz,el,en-GB,en-ZA,eo,es,et,eu,fa,fi,fr,ga,gd,gl,gu,he,hi,hr,hu,is,it,ja,ka,kk,km,kn,ko,kok,ks,ku,ky,lo,lt,lv,mai,mk,ml,mn,mni,mr,ms,my,ns,oc,om,or,pa-IN,pap,pl,ps,pt,ro,ru,rw,sa-IN,sat,sd,sh,si,sk,sl,sq,sr,ss,st,sv,sw-TZ,ta,te,tg,th,ti,tn,tr,ts,ug,uk,ur,uz,ve,vi,xh,zh-CN,zh-TW,zu}.tar.gz
#all of locales. Paradize for linguists!
LOCALES="af
ar
as
be-BY
bg
bn
bo
br
brx
bs
ca
cs
cy
da
de
dgo
dz
el
en-GB
en-ZA
eo
es
et
eu
fa
fi
fr
ga
gd
gl
gu
he
hi
hr
hu
is
it
ja
ka
kk
km
kn
ko
kok
ks
ku
ky
lo
lt
lv
mai
mk
ml
mn
mni
mr
ms
my
ns
oc
om
or
pa-IN
pap
pl
ps
pt
ro
ru
rw
sa-IN
sat
sd
sh
si
sk
sl
sq
sr
ss
st
sv
sw-TZ
ta
te
tg
th
ti
tn
tr
ts
ug
uk
ur
uz
ve
vi
xh
zh-CN
zh-TW
zu"
for i in $LOCALES ; do
	cd $startdir/build/$i
	tar xfv $srcache/LibO_${ver/-/}_Linux_x86_langpack-rpm_$i.tar.gz || exit 1
	tar xfv $srcache/LibO_${ver/-/}_Linux_x86_helppack-rpm_$i.tar.gz || exit 1
	cd $startdir/build/$i/LibO_${ver/-/}_Linux_x86_langpack-rpm_$i/RPMS/ || exit 1
	rm libreoffice3-dict-*.rpm || /bin/true
	for k in *.rpm ; do
		rpmextract $k
	done
	rpmextract $startdir/build/$i/LibO_3.3.0rc4_Linux_x86_helppack-rpm_$i/RPMS/libobasis3.3-$i-help-3.3.0-6.i586.rpm
cp -R opt/libreoffice/basis$tree/* $startdir/libreoffice_package_$i/usr/lib${LIBDIRSUFFIX}/libreoffice/basis-link || exit 1
cp -R opt/libreoffice/{program,readmes} $startdir/libreoffice_package_$i/usr/lib${LIBDIRSUFFIX}/libreoffice || exit 1
chown root:root -R $startdir/libreoffice_package_$i/usr/lib${LIBDIRSUFFIX}/libreoffice || exit 1
done
}


#ran after function build() 
#после сборки
after_build()
{
echo ""
}
